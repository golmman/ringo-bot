{
    "extends": "airbnb-base",
    "env": {
        "browser": true,
        "node": true,
        "mocha": true
    },
    "rules": {
        "indent": [
            "error",
            4,
            {
                "SwitchCase": 1
            }
        ],
        "no-bitwise": "off",
        "no-labels": "off",
        "no-mixed-operators": "off",
        "no-param-reassign": "off",
        "no-restricted-syntax": "off",
        "prefer-destructuring": "off"
    }
}
